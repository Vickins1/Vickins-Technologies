   <style>
	/* Preloader Background */
	.preloader {
	    position: fixed;
	    top: 0;
	    left: 0;
	    width: 100%;
	    height: 100%;
	    background: linear-gradient(90deg, #1e40af, #60a5fa);
	    display: flex;
	    align-items: center;
	    justify-content: center;
	    z-index: 9999;
	    overflow: hidden;
	    perspective: 1200px; /* Adds 3D depth */
	}
   
	/* Unwrap Effect Container */
	.unwrap-effect {
	    position: relative;
	    width: 100%;
	    height: 100%;
	    display: flex;
	}
   
	/* Unwrap Sections */
	.unwrap-section {
	    position: absolute;
	    width: 25%; /* Four vertical sections */
	    height: 100%;
	    background: linear-gradient(180deg, #ffffff, #e5e7eb);
	    box-shadow: 2px 0 10px rgba(0, 0, 0, 0.1);
	    transition: transform 0.6s cubic-bezier(0.68, -0.55, 0.27, 1.55), opacity 0.6s ease-out;
	    transform-origin: top;
	    border-right: 1px solid #d4d4d8;
	}
   
	.unwrap-section--1 { left: 0; }
	.unwrap-section--2 { left: 25%; }
	.unwrap-section--3 { left: 50%; }
	.unwrap-section--4 { left: 75%; }
   
	/* Loaded State for Sequential Upward Animation */
	.preloader.loaded .unwrap-section--1 {
	    transform: translateY(-100%) rotateX(90deg);
	    opacity: 0;
	    transition-delay: 0s;
	}
   
	.preloader.loaded .unwrap-section--2 {
	    transform: translateY(-100%) rotateX(90deg);
	    opacity: 0;
	    transition-delay: 0.3s;
	}
   
	.preloader.loaded .unwrap-section--3 {
	    transform: translateY(-100%) rotateX(90deg);
	    opacity: 0;
	    transition-delay: 0.6s;
	}
   
	.preloader.loaded .unwrap-section--4 {
	    transform: translateY(-100%) rotateX(90deg);
	    opacity: 0;
	    transition-delay: 0.9s;
	}
   
	/* Loader Core */
	.loader-core {
	    position: absolute;
	    top: 50%;
	    left: 50%;
	    transform: translate(-50%, -50%);
	    z-index: 1;
	    display: flex;
	    flex-direction: column;
	    align-items: center;
	    opacity: 1;
	    transition: opacity 0.5s ease-out 1.5s;
	}
   
	.preloader.loaded .loader-core {
	    opacity: 0;
	}
   
	/* Loader Spinner */
	.loader-spinner {
	    width: 40px;
	    height: 40px;
	    border: 4px solid transparent;
	    border-top-color: #ffffff;
	    border-right-color: #ffffff;
	    border-radius: 50%;
	    animation: spin 0.9s linear infinite;
	    margin-bottom: 10px;
	}
   
	/* Loader Text */
	.loader-text {
	    color: #ffffff;
	    font-family: 'Arial', sans-serif;
	    font-size: 1.4rem;
	    font-weight: 700;
	    letter-spacing: 2px;
	    text-transform: uppercase;
	    animation: pulse 2s infinite ease-in-out;
	}
   
	/* Animations */
	@keyframes spin {
	    0% { transform: rotate(0deg); }
	    100% { transform: rotate(360deg); }
	}
   
	@keyframes pulse {
	    0%, 100% { opacity: 1; }
	    50% { opacity: 0.5; }
	}
   
	/* Responsive Adjustments */
	@media (max-width: 768px) {
	    .loader-text {
		 font-size: 1.1rem;
	    }
	    .loader-spinner {
		 width: 32px;
		 height: 32px;
	    }
	    .unwrap-section {
		 transition-duration: 0.5s; /* Slightly faster on mobile */
	    }
	}
   </style>
   
