	<style>
		/* Modern Switch Styles */
		.modern-switch {
			position: relative;
			display: inline-block;
			width: 50px;
			height: 24px;
		}

		.modern-switch input {
			opacity: 0;
			width: 0;
			height: 0;
		}

		.modern-slider {
			position: absolute;
			cursor: pointer;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			background-color: #ccc;
			border-radius: 24px;
			transition: 0.4s;
		}

		.modern-slider::before {
			position: absolute;
			content: "";
			height: 18px;
			width: 18px;
			left: 3px;
			bottom: 3px;
			background-color: white;
			border-radius: 50%;
			transition: 0.4s;
		}

		input:checked+.modern-slider {
			background-color: #0071dc;
		}

		input:checked+.modern-slider::before {
			transform: translateX(26px);
		}
	</style>

